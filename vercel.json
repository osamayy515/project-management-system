{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "django": {
          "static": "staticfiles",
          "build": "python3.9 manage.py collectstatic --noinput"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/<all*>",
      "dest": "wsgi.py"
    }
  ]
}